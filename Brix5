import java.util.*;
public class App {
    public static void main(String args[]) {
        Scanner s = new  Scanner(System.in);
        System.out.print("Enter any year:");

        int year = s.nextInt();

        boolean birth = false;

        if (year % 4 == 0) {
            birth = true;
        } else if (year % 100 == 0) {
            birth = false;

        } else if (year % 4 == 0) {
            birth = true;
        } else {
            System.out.println("Year " + year + " is a leap Year");
        }
        System.out.println("Year " + year + " is not a leap Year");
        




    }
}
